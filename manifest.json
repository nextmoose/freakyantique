{
    "manifest_version": 2,
    "name": "Court Scraper",
    "version": "0.0.0",
    "background": {
        "scripts":["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["https://eapps.courts.state.va.us/gdcourts/caseSearch.do"],
        "js": ["content.js"]
    }],
    "permissions": [
        { "fileSystem": ["write", "retainEntries", "directory"] }
    ]
}
