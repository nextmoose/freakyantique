{
    "manifest_version": 2,
    "name": "Court Scraper",
    "version": "0.0.0",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["https://eapps.courts.state.va.us/gdcourts/caseSearch.do?welcomePage=welcomePage", "https://eapps.courts.state.va.us/gdcourts/changeCourt.do*"],
        "js": ["common.js", "welcome.js"]
        }, {
        "matches": ["https://eapps.courts.state.va.us/gdcourts/caseSearch.do*"],
        "js": ["common.js", "caseSearch.js"]
    }, {
        "matches": ["https://eapps.courts.state.va.us/gdcourts/criminalDetail.do*"],
        "js": ["common.js", "criminalDetail.js"]
    }, {
        "matches": ["https://eapps.courts.state.va.us/gdcourts/captchaReverification.do*"],
        "js": ["common.js", "captchaReverification.js"]
    }]
}
